<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <filter>
        <filter-name>ManagerFilter</filter-name>
        <filter-class>com.zqy.filter.ManagerFilter</filter-class>

    </filter>

    <filter-mapping>
        <filter-name>ManagerFilter</filter-name>
        <url-pattern>/group_edit.jsp</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>ManagerFilter</filter-name>
        <url-pattern>/group_pay.jsp</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>ManagerFilter</filter-name>
        <url-pattern>/group_pay_success.jsp</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>ManagerFilter</filter-name>
        <url-pattern>/group_update.jsp</url-pattern>
    </filter-mapping>



</web-app>